{
  "extends": ["eslint:recommended", "react-app", "prettier"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "keyword-spacing": 2,
    "no-unused-vars": "warn",
    "no-trailing-spaces": "warn"
  },
  "overrides": [
    {
      "files": ["*.jsx", "*.tsx"]
    }
  ]
}
